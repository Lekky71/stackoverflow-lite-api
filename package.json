{
  "name": "so-lite-api",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/app.js",
  "scripts": {
    "start": "nodemon app.js --exec babel-node --presets es2015,stage-2",
    "build": "babel --source-maps --out-dir=dist src",
    "serve": "node dist/app.js",
    "server": "npm run build && npm run serve",
    "test": "npm run build && mocha ./dist/tests/*.js --compilers js:babel-register --reporter spec --timeout 3000"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "express-validator": "^5.1.2",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.3.0",
    "morgan": "~1.9.0",
    "pg": "^7.4.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^3.5.0",
    "chai-http": "^4.2.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^3.5.3"
  }
}
